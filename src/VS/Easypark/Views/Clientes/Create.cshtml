@model Easypark.Models.Cliente

@{
    ViewData["Title"] = "Create";
}

<h1>Cadastrar Cliente</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="nome" class="control-label"></label>
                <input asp-for="nome" class="form-control" />
                <span asp-validation-for="nome" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="tipoCarro" class="control-label"></label>
                <input asp-for="tipoCarro" class="form-control" />
                <span asp-validation-for="tipoCarro" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="modeloCarro" class="control-label"></label>
                <input asp-for="modeloCarro" class="form-control" />
                <span asp-validation-for="modeloCarro" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="placaCarro" class="control-label"></label>
                <input asp-for="placaCarro" class="form-control" />
                <span asp-validation-for="placaCarro" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CNPJ" class="control-label"></label>
                <input asp-for="CNPJ" class="form-control" />
                <span asp-validation-for="CNPJ" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CPF" class="control-label"></label>
                <input asp-for="CPF" class="form-control" />
                <span asp-validation-for="CPF" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Cadastrar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
